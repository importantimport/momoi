{
  "name": "@momoi/css",
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "exports": {
    "./*": {
      "types": "./dist/*.css.d.mts",
      "import": "./dist/*.css.mjs"
    },
    "./*.js": {
      "types": "./dist/*.css.d.mts",
      "import": "./dist/*.css.mjs"
    },
    "./*.mjs": {
      "types": "./dist/*.css.d.mts",
      "import": "./dist/*.css.mjs"
    },
    "./*.css": "./dist/*.css.css",
    "./dist/*": {
      "types": "./dist/*.css.d.mts",
      "import": "./dist/*.css.mjs"
    },
    "./dist/*.mjs": {
      "types": "./dist/*.css.d.mts",
      "import": "./dist/*.css.mjs"
    },
    "./dist/*.css": "./dist/*.css.css",
    "./src/*": "./src/*"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "devDependencies": {
    "@radix-ui/colors": "^2.1.0"
  },
  "dependencies": {
    "@vanilla-extract/recipes": "^0.5.0"
  }
}
