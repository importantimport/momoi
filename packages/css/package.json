{
  "name": "@momoi/css",
  "version": "0.0.0",
  "files": ["dist"],
  "exports": {
    "./*": {
      "types": "./dist/*.css.d.mts",
      "import": "./dist/*.css.mjs"
    },
    "./*.js": {
      "types": "./dist/*.css.d.mts",
      "import": "./dist/*.css.mjs"
    },
    "./*.mjs": {
      "types": "./dist/*.css.d.mts",
      "import": "./dist/*.css.mjs"
    },
    "./*.css": "./dist/*.css.css"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "devDependencies": {
    "@radix-ui/colors": "^2.1.0"
  }
}
