{
  "name": "@momoi/css",
  "type": "module",
  "version": "0.0.0",
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    },
    "./*.js": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    },
    "./*.css": "./dist/*.css",
    "./dist/*.js": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    },
    "./dist/*.css": "./dist/*.css.css",
    "./src/*": "./src/*"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "devDependencies": {
    "@radix-ui/colors": "^2.1.0"
  }
}
