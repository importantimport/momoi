{
  "name": "@momoi/react",
  "type": "module",
  "version": "0.0.0",
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    },
    "./*.js": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    },
    "./*.css": "./dist/*.css",
    "./src/*": "./src/*"
  },
  "files": ["dist", "src"],
  "scripts": {
    "build": "tsup",
    "build:docs": "ladle build",
    "dev": "tsup --watch",
    "dev:docs": "ladle serve",
    "preview:docs": "ladle preview"
  },
  "dependencies": {
    "@radix-ui/react-form": "^0.0.3",
    "@radix-ui/react-slot": "^1.0.2"
  },
  "devDependencies": {
    "@ladle/react": "^2.17.2",
    "@momoi/css": "workspace:*"
  }
}
